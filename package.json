        "lint:fix": "eslint . --fix",
        "test": "jest --watch --runInBand",
        "prepare": "husky",
        "start": "ts-node src/index.ts",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:generate": "typeorm-ts-node-commonjs migration:generate",
        "migration:run": "typeorm-ts-node-commonjs migration:run",
        "migration:create": "typeorm-ts-node-commonjs migration:create"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "A user management service",
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^5.0.0",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.14",
        "@types/node": "^16.11.10",
        "@types/supertest": "^6.0.3",
        "@types/winston": "^2.4.4",
        "cross-env": "^7.0.3",
        "eslint": "^9.22.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.5.0",
        "mock-jwks": "^1.0.10",
        "nodemon": "^3.1.9",
        "prettier": "3.5.3",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
    },
    "lint-staged": {
        "*.ts": [
            "npm run format:fix",
            "npm run lint:fix"
        ]
    },
    "dependencies": {
        "@types/cookie-parser": "^1.4.9",
        "@types/jsonwebtoken": "^9.0.10",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.7",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-jwt": "^8.5.1",
        "express-validator": "^7.2.1",
        "http-error": "^0.0.6",
        "jsonwebtoken": "^9.0.2",
